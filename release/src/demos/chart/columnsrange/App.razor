<div id="chartContainer">
    <JqxChart
        title="Logarithmic Column Range" description="logarithmic scale with base 2"
        enableAnimations=true padding=padding titlePadding=titlePadding source=sampleData 
        xAxis=xAxis valueAxis=valueAxis seriesGroups=seriesGroups colorScheme="scheme07">
    </JqxChart>
</div>

@code {
    IDictionary<string, object>[] sampleData = new Dictionary<string, object>[]
    {
        new Dictionary<string, object>() { { "a", 0.35 }, { "b", 14.5 }, { "c", 0.35 }, { "d", 0.1 } }, 
        new Dictionary<string, object>() { { "a", 1 }, { "b", 2.5 }, { "c", 1}, { "d", 0.2 } },
        new Dictionary<string, object>() { { "a", 10 }, { "b", 0.2 }, { "c", 10 }, { "d", 50 } },
        new Dictionary<string, object>() { { "a", 100 }, { "b", 205 }, { "c", 100 }, { "d", 40 } },
        new Dictionary<string, object>() { { "a", 1 }, { "b", 100 }, { "c", 1 }, { "d", 200 } },
        new Dictionary<string, object>() { { "a", 5.11 }, { "b", 10.13 }, { "c", 5.11 }, { "d", 0.2 } },
        new Dictionary<string, object>() { { "a", 20.13 }, { "b", 10.13 }, { "c", 20.3 }, { "d", 4 } }
    };

    IDictionary<string, int> padding = new Dictionary<string, int>()
    {
        { "left", 5 },
        { "top", 5 },
        { "right", 5 },
        { "bottom", 5 }
    };

    IDictionary<string, int> titlePadding = new Dictionary<string, int>()
    {
        { "left", 0 },
        { "top", 0 },
        { "right", 0 },
        { "bottom", 10 }
    };    

    IDictionary<string, object> xAxis = new Dictionary<string, object>()
    {
        { 
            "gridLines",
            new Dictionary<string, object>()
            {
                { "dashStyle", "2,2" }
            } 
        },
        {
            "tickMarks",
            new Dictionary<string, object>()
            {
                { "dashStyle", "1,2" }
            } 
        }
    };

    IDictionary<string, object> valueAxis = new Dictionary<string, object>()
    {
        { "logarithmicScale", true },
        { "logarithmicScaleBase", 2 },
        { "unitInterval", 1 },
        {
            "title", 
            new Dictionary<string, object>()
            {
                { "text", "Value" }
            }        
        },
        {
            "labels",
            new Dictionary<string, object>()
            {
                { 
                    "formatSettings",
                    new Dictionary<string, object>()
                    {
                        { "decimalPlaces", 3 }
                    } 
                },
                { "horizontalAlignment", "right" }
            }
        },
        { 
            "gridLines",
            new Dictionary<string, object>()
            {
                { "dashStyle", "2,2" }
            } 
        },
        { 
            "tickMarks",
            new Dictionary<string, object>()
            {
                { "dashStyle", "1,2" }
            } 
        }
    };

    List<Dictionary<string, object>> seriesGroups = new List<Dictionary<string, object>>()
    {      
        new Dictionary<string, object>() 
        { 
            { "type", "rangecolumn" },           
            {
                "series", 
                new Dictionary<string, object>[]
                {
                    new Dictionary<string, object>() 
                    { 
                        { "dataFieldFrom", "a" },
                        { "dataFieldTo", "b" }, 
                        { "displayText", "from A to B" }
                    },
                    new Dictionary<string, object>() 
                    { 
                        { "dataFieldFrom", "c" },
                        { "dataFieldTo", "d" }, 
                        { "displayText", "from C to D" }
                    }
                }
            }
        }
    };
}

<style>
    #chartContainer > div {
        width: 800px;
        height: 500px;
    }
</style>
