<div id="chartContainer">
    <JqxChart
        title="Weather in Geneva, Switzerland" 
        description="Climatological Information about Geneva"
        showLegend=true enableAnimations=true padding=padding enableCrosshairs=true
        titlePadding=titlePadding source=data xAxis=xAxis valueAxis=valueAxis
        seriesGroups=seriesGroups colorScheme="scheme05">
    </JqxChart>
</div>

@code {
    IDictionary<string, object>[] data = new Dictionary<string, object>[]
    {
        new Dictionary<string, object>() 
        {
            { "month", "Jan" }, { "min", -1.9 }, { "max", 3.7 }, { "avg", 0.2 }
        }, 
        new Dictionary<string, object>() 
        { 
            { "month", "Feb" }, { "min", -0.9 }, { "max", 5.9 }, { "avg", 1.1 }
        },
        new Dictionary<string, object>() 
        { 
            { "month", "Mar" }, { "min", 0.8 }, { "max", 9.8 }, { "avg", 4.9 }
        },
        new Dictionary<string, object>() 
        { 
            { "month", "Apr" }, { "min", 4.1 }, { "max", 13.9 }, { "avg", 8.7 }
        },
        new Dictionary<string, object>() 
        { 
            { "month", "Apr" }, { "min", 8.0 }, { "max", 18.4 }, { "avg", 13.1 }
        },
        new Dictionary<string, object>() 
        { 
            { "month", "May" }, { "min", 11.3 }, { "max", 22.2 }, { "avg", 16.6 }
        }, 
        
        new Dictionary<string, object>() 
        { 
            { "month", "Jul" }, { "min", 13.3 }, { "max", 25.3 }, { "avg", 18.4 }
        },
        new Dictionary<string, object>() 
        { 
            { "month", "Aug" }, { "min", 13.3 }, { "max", 24.4 }, { "avg", 17.6 }
        },
        new Dictionary<string, object>() 
        { 
            { "month", "Sep" }, { "min", 10.3 }, { "max", 20.8 }, { "avg", 14.3 }
        }, 
        new Dictionary<string, object>() 
        { 
            { "month", "Oct" }, { "min", 6.6 }, { "max", 14.9 }, { "avg", 9.2 }
        },
        new Dictionary<string, object>() 
        { 
            { "month", "Nov" }, { "min", 2.1 }, { "max", 8.4 }, { "avg", 4.2 }
        },
        new Dictionary<string, object>() 
        { 
            { "month", "Dec" }, { "min", -0.5 }, { "max", 4.5 }, { "avg", 1.5 }
        }
    };

    IDictionary<string, int> padding = new Dictionary<string, int>()
    {
        { "left", 5 },
        { "top", 5 },
        { "right", 5 },
        { "bottom", 5 }
    };

    IDictionary<string, int> titlePadding = new Dictionary<string, int>()
    {
        { "left", 90 },
        { "top", 0 },
        { "right", 0 },
        { "bottom", 10 }
    };    

    IDictionary<string, object> xAxis = new Dictionary<string, object>()
    {
        { "dataField", "month" },
        { "unitInterval", 1 },          
        { 
            "gridLines", 
            new Dictionary<string, object>()
            {
                { "step", 3 }
            }
        }
    };

    IDictionary<string, object> valueAxis = new Dictionary<string, object>()
    {
        { "minValue", -5 },
        { "maxValue", 30 },
        { "unitInterval", 5 },
        { 
            "title", 
            new Dictionary<string, string>()
            {
                { "text", "Temperature [C]<br>" }
            }
        },        
        {
            "labels", 
            new Dictionary<string, object>()
            {
                { "horizontalAlignment", "right" },
                { 
                    "formatSettings", 
                    new Dictionary<string, string>()
                    {
                        { "sufix", "°" }
                    }
                }            
            }
        }
    };

    IDictionary<string, object>[] seriesGroups = new Dictionary<string, object>[]
    {      
        new Dictionary<string, object>() 
        { 
            { "type", "rangecolumn" }, 
            { "columnsGapPercent", 50 },
            {
                "series", 
                new Dictionary<string, object>[]
                {
                    new Dictionary<string, object>() 
                    { 
                        { "dataFieldTo", "max" }, 
                        { "displayText", "Temperature Range" },
                        { "dataFieldFrom", "min" },
                        { "opacity", 1 }
                    }
                }
            }
        },
        new Dictionary<string, object>() 
        { 
            { "type", "spline" },
            {
                "series", 
                new Dictionary<string, object>[]
                {
                    new Dictionary<string, object>() 
                    { 
                        { "dataField", "avg" }, 
                        { "displayText", "Average Temperature" },
                        { "opacity", 1 },
                        { "lineWidth", 2 }
                    }
                }
            }
        }
    };
}

<style>
    #chartContainer > div {
        width: 800px;
        height: 500px;
    }
</style>
