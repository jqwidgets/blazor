<div id="chartContainer">
    <JqxChart
        title="Logarithmic Spline Area Range" description="logarithmic scale with base 2"
        enableAnimations=true padding=padding titlePadding=titlePadding
        source=sampleData xAxis=xAxis valueAxis=valueAxis seriesGroups=seriesGroups>
    </JqxChart>
</div>

@code {
    IDictionary<string, object>[] sampleData = new Dictionary<string, object>[]
    {
        new Dictionary<string, object>() { { "a", 0.35 }, { "b", 14.5 } }, 
        new Dictionary<string, object>() { { "a", 1 }, { "b", 2.5 } },
        new Dictionary<string, object>() { { "a", 10 }, { "b", 0.2 } },
        new Dictionary<string, object>() { { "a", 100 }, { "b", 205 } },
        new Dictionary<string, object>() { { "a", 1 }, { "b", 100 } },
        new Dictionary<string, object>() { { "a", 5.11 }, { "b", 10.13 } },
        new Dictionary<string, object>() { { "a", 20.13 }, { "b", 10.13 } },
        new Dictionary<string, object>() { { "a", 600 }, { "b", 300 } }
    };

    IDictionary<string, int> padding = new Dictionary<string, int>()
    {
        { "left", 5 },
        { "top", 5 },
        { "right", 5 },
        { "bottom", 5 }
    };

    IDictionary<string, int> titlePadding = new Dictionary<string, int>()
    {
        { "left", 0 },
        { "top", 0 },
        { "right", 0 },
        { "bottom", 10 }
    };    

    IDictionary<string, object> xAxis = new Dictionary<string, object>()
    {
        { "valuesOnTicks", false },
        { 
            "gridLines",
            new Dictionary<string, object>()
            {
                { "visible", false }
            } 
        }
    };

    IDictionary<string, object> valueAxis = new Dictionary<string, object>()
    {
        { "logarithmicScale", true },
        { "logarithmicScaleBase", 2 },
        { "unitInterval", 1 },
        { "flip", false },
        {
            "title", 
            new Dictionary<string, object>()
            {
                { "text", "Value" }
            }        
        },
        {
            "labels",
            new Dictionary<string, object>()
            {
                { 
                    "formatSettings",
                    new Dictionary<string, object>()
                    {
                        { "decimalPlaces", 3 }
                    } 
                },
                { "horizontalAlignment", "right" }
            }
        },
        { 
            "gridLines",
            new Dictionary<string, object>()
            {
                { "visible", false }
            } 
        }
    };

    List<Dictionary<string, object>> seriesGroups = new List<Dictionary<string, object>>()
    {      
        new Dictionary<string, object>() 
        { 
            { "type", "splinerangearea" },           
            {
                "series", 
                new Dictionary<string, object>[]
                {
                    new Dictionary<string, object>() 
                    { 
                        { "dataFieldFrom", "a" },
                        { "dataFieldTo", "b" }, 
                        { "displayText", "from A to B" },
                        { "opacity", 0.8 },
                        { "lineWidth", 1 }
                    }
                }
            }
        }
    };
}

<style>
    #chartContainer > div {
        width: 800px;
        height: 500px;
    }
</style>
