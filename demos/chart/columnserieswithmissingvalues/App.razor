<div id="chartContainer">
    <JqxChart
        title="CPI inflation comparison by country" description="Years: 2018 vs 2019"
        showLegend=true enableAnimations=false padding=padding titlePadding=titlePadding
        source=sampleData xAxis=xAxis valueAxis=valueAxis seriesGroups=seriesGroups 
        colorScheme="scheme02">
    </JqxChart>
</div>

@code {
    IDictionary<string, object>[] sampleData = new Dictionary<string, object>[]
    {
        new Dictionary<string, object>() { { "Hour", 1 }, { "Sales", 135 } }, 
        new Dictionary<string, object>() { { "Hour", 3 }, { "Sales", 145 } },
        new Dictionary<string, object>() { { "Hour", 5 }, { "Sales", 90 } },
        new Dictionary<string, object>() { { "Hour", 15 }, { "Sales", 66 } },
        new Dictionary<string, object>() { { "Hour", 17 }, { "Sales", 43 } },
        new Dictionary<string, object>() { { "Hour", 18 }, { "Sales", 122 } },
        new Dictionary<string, object>() { { "Hour", 22 }, { "Sales", 59 } },
        new Dictionary<string, object>() { { "Hour", 23 }, { "Sales", 70 } }
    };

    IDictionary<string, int> padding = new Dictionary<string, int>()
    {
        { "left", 5 },
        { "top", 5 },
        { "right", 10 },
        { "bottom", 5 }
    };

    IDictionary<string, int> titlePadding = new Dictionary<string, int>()
    {
        { "left", 90 },
        { "top", 0 },
        { "right", 0 },
        { "bottom", 10 }
    };    

    IDictionary<string, object> xAxis = new Dictionary<string, object>()
    {
        { "dataField", "Hour" },
        { "minValue", 0 },
        { "maxValue", 23 },
        { "unitInterval", 1 },
        { "valuesOnTicks", false },
        { 
            "labels", 
            new Dictionary<string, object>() 
            {
                { "angle", 0 }
            }
        },
        { 
            "tickMarks", 
            new Dictionary<string, object>() 
            {
                { "visible", true },
                { "interval", 1 }
            }
        },
        { 
            "gridLines", 
            new Dictionary<string, object>() 
            {
                { "visible", true },
                { "interval", 3 }
            }
        }
    };

    IDictionary<string, object> valueAxis = new Dictionary<string, object>()
    {
        { "visible", true },
        { "minValue", 0 },
        { "maxValue", 200 },
        { "unitInterval", 50 },
        { 
            "title", 
            new Dictionary<string, object>()
            {
                { "text", "Sales ($)<br>" }
            }        
        },                
        { 
            "labels", 
            new Dictionary<string, object>()
            {
                { "horizontalAlignment", "right" }
            }
        }
    };

    List<Dictionary<string, object>> seriesGroups = new List<Dictionary<string, object>>()
    {      
        new Dictionary<string, object>() 
        { 
            { "type", "column" },
            {
                "series", 
                new Dictionary<string, object>[]
                {
                    new Dictionary<string, object>() 
                    { 
                        { "dataField", "Sales" }, { "displayText", "Sales" }, { "showLabels", true }
                    }
                }
            }
        }
    };
}

<style>
    #chartContainer > div {
        width: 800px;
        height: 500px;
    }
</style>
