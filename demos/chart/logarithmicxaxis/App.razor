<div id="chartContainer">
    <JqxChart
        title="Logarithmic Scale x-Axis Example"
        description="Horizontal axis with base 2 logarithmic scale"
        enableAnimations=true padding=padding titlePadding=titlePadding
        source=sampleData xAxis=xAxis valueAxis=valueAxis seriesGroups=seriesGroups>
    </JqxChart>
</div>

@code {
    IDictionary<string, object>[] sampleData = new Dictionary<string, object>[]
    {
        new Dictionary<string, object>() { { "a", 0.1535 }, { "b", 0.5 } }, 
        new Dictionary<string, object>() { { "a", 0.48 }, { "b", 20.5 } },
        new Dictionary<string, object>() { { "a", 10 }, { "b", 60 } },
        new Dictionary<string, object>() { { "a", 100 }, { "b", 80 } },
        new Dictionary<string, object>() { { "a", 200 }, { "b", 90 } },
        new Dictionary<string, object>() { { "a", 245.11 }, { "b", 100.13 } },
        new Dictionary<string, object>() { { "a", 300.13 }, { "b", 150.13 } },
        new Dictionary<string, object>() { { "a", 340 }, { "b", 200 } }
    };

    IDictionary<string, int> padding = new Dictionary<string, int>()
    {
        { "left", 15 },
        { "top", 5 },
        { "right", 15 },
        { "bottom", 5 }
    };

    IDictionary<string, int> titlePadding = new Dictionary<string, int>()
    {
        { "left", 0 },
        { "top", 0 },
        { "right", 0 },
        { "bottom", 10 }
    };    

    IDictionary<string, object> xAxis = new Dictionary<string, object>()
    {
        { "dataField", "a" },
        { "logarithmicScale", true },
        { "logarithmicScaleBase", 2 }
    };

    IDictionary<string, object> valueAxis = new Dictionary<string, object>()
    {
        { "logarithmicScale", false },
        { "logarithmicScaleBase", 2 },  
        { 
            "title", 
            new Dictionary<string, object>()
            {
                { "text", "Value" }
            }        
        },
        {
            "labels",
            new Dictionary<string, object>()
            {
                { "horizontalAlignment", "right" },
            }
        }
    };

    List<Dictionary<string, object>> seriesGroups = new List<Dictionary<string, object>>()
    {      
        new Dictionary<string, object>() 
        { 
            { "type", "line" },           
            {
                "series", 
                new Dictionary<string, object>[]
                {
                    new Dictionary<string, object>() 
                    { 
                        { "dataField", "a" }, 
                        { "displayText", "A" },
                        { "symbolType", "square" },
                        { "symbolSize", 6 },
                        { "dashStyle", "4.4" },
                        { "lineWidth", 1 }
                    },
                    new Dictionary<string, object>() 
                    { 
                        { "dataField", "b" }, 
                        { "displayText", "B" },
                        { "symbolType", "circle" },
                        { "symbolSize", 6 },
                        { "lineWidth", 1 }
                    }
                }
            }
        }
    };
}

<style>
    #chartContainer > div {
        width: 800px;
        height: 500px;
    }
</style>
