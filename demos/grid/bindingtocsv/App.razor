<JqxGrid
    width=850 source=source columns=columns columnsresize=true>
</JqxGrid>

@code {

    IDictionary<string, object> source = new Dictionary<string, object>()
    {
        { "datatype", "csv" },
        { 
            "dataFields",
            new Dictionary<string, string>[]
            {
                new Dictionary<string, string>() { { "name", "Date" }, { "type", "date" } },
                new Dictionary<string, string>() { { "name", "S&P 500" }, { "type", "float" } },
                new Dictionary<string, string>() { { "name", "NASDAQ" }, { "type", "float" } }
            }
        },        
        { "url", "./sampledata/nasdaq_vs_sp500.txt" }
    };

    IDictionary<string, object>[] columns = new Dictionary<string, object>[]
    {
        new Dictionary<string, object>() 
        {
            { "text", "Date" }, { "datafield", "Date" }, { "cellsformat", "D" }, { "width", 250 }
        },
        new Dictionary<string, object>() 
        {
            { "text", "S&P 500" }, { "datafield", "S&P 500" }, { "width", 300 }, { "cellsformat", "f" }
        },
        new Dictionary<string, object>() 
        {
            { "text", "NASDAQ" }, { "datafield", "NASDAQ" }, { "cellsformat", "f" }
        }
    };
}
